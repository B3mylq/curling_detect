<launch>
    <!-- 标定点1在基坐标系下的位置 -->
    <param name="marker1_x" value="11.888" type="double" />
    <param name="marker1_y" value="0" type="double" />
    <!-- 标定点2在基坐标系下的位置 -->
    <param name="marker2_x" value="11.888" type="double" />
    <param name="marker2_y" value="4.950" type="double" />
    <!-- 雷达在基坐标系下的位置 -->
    <param name="lidar_x" value="15.888" type="double" />
    <param name="lidar_y" value="-2" type="double" />

    <node pkg="curling_detect" type="prompt_graph" name="prompt_graph" output="screen" />
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find curling_detect)/config/prompt_graph.rviz" required="true"/>

</launch>